<?xml version="1.0" encoding="utf-8"?>
<configuration>

	<configSections>
		<section name="system.data" type="System.Data.Common.DbProviderFactoriesConfigurationHandler, Microsoft.Practices.EnterpriseLibrary.Data, Version=6.0.0.0, Culture=neutral, PublicKeyToken=null"/>
	</configSections>
	
<connectionStrings>
		<add name="Local" connectionString="host=localhost;database=testproject;User id=root;password=123456789;charset=utf8mb4;SslMode=none" providerName="MySql.Data.MySqlClient" />
